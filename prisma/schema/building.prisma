model Building {
  id            Int             @id @default(autoincrement())
  name          String
  address       String
  floors        Int
  MediaBuilding MediaBuilding[]
  Room          Room[]
}

model Room {
  id         Int      @id @default(autoincrement())
  floor      Int
  building   Building @relation(fields: [buildingId], references: [id])
  buildingId Int
}
